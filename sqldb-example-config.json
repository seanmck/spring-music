{
  "resourceGroup": "Cloud_Practice_Lab_West",
  "location": "westus",
  "sqlServerName": "demo-azure-sqlserver",
  "sqlServerParameters": {
    "allowSqlServerFirewallRule": [
      {
      "ruleName": "labd-sqlserver-rule",
      "startIpAddress": "0.0.0.0",
      "endIpAddress": "255.255.255.255"
      }
    ],
    "properties": {
      "administratorLogin": "labdsql",
      "administratorLoginPassword": "Welcome1"
    }
  },
  "sqldbName": "labdsqldb",
  "sqldbParameters": {
    "properties": {
      "collation": "SQL_Latin1_General_CP1_CI_AS"
    }
  }
}
